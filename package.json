{
  "name": "@navios/commander-mono",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "turbo run build --filter='./packages/*'",
    "check": "turbo run check --filter='./packages/*'",
    "lint": "turbo run lint --filter='./packages/*'",
    "test": "turbo run test --filter='./packages/*'",
    "test:ci": "turbo run test:ci --filter='./packages/*'",
    "publish:all": "turbo run publish:pkg --filter='./packages/*'",
    "publish:next": "turbo run publish:next --filter='./packages/*'"
  },
  "devDependencies": {
    "@navios/builder": "^1.0.0",
    "@swc/core": "^1.15.8",
    "@types/bun": "^1.3.6",
    "@types/node": "25.0.9",
    "@typescript/native-preview": "^7.0.0-dev.20260115.1",
    "@vitest/coverage-v8": "^4.0.17",
    "oxfmt": "^0.24.0",
    "oxlint": "^1.39.0",
    "tsdown": "^0.19.0",
    "tslib": "^2.8.1",
    "turbo": "^2.7.4",
    "typescript": "^5.9.3",
    "unplugin-swc": "^1.5.9",
    "vitest": "^4.0.17",
    "zod": "^4.3.5"
  },
  "packageManager": "yarn@4.12.0"
}
