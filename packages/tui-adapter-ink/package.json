{
  "name": "@navios/tui-adapter-ink",
  "version": "1.2.1",
  "license": "MIT",
  "author": {
    "name": "Oleksandr Hanzha",
    "email": "alex@granted.name"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navios-org/commander.git",
    "directory": "packages/tui-adapter-ink"
  },
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "typings": "./lib/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./lib/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      }
    }
  },
  "scripts": {
    "check": "yarn run -T tsgo --build",
    "lint": "yarn run -T oxlint --fix && yarn run -T oxfmt",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:watch": "vitest",
    "build": "yarn run -T tsdown",
    "publish:pkg": "yarn npm publish --access public",
    "publish:next": "yarn npm publish --access public --tag next"
  },
  "dependencies": {
    "ink": "^6.6.0",
    "ink-syntax-highlight": "^2.0.2",
    "react": "^19.2.3"
  },
  "devDependencies": {
    "@navios/commander-tui": "workspace:*",
    "@testing-library/react": "^16.3.1",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "happy-dom": "^20.3.1",
    "ink-testing-library": "^4.0.0",
    "react-dom": "^19.2.3",
    "strip-ansi": "^7.1.0",
    "typescript": "^5.9.3",
    "unplugin-swc": "^1.5.9",
    "vitest": "^4.0.17"
  },
  "peerDependencies": {
    "@navios/commander-tui": "^1.3.0",
    "@navios/core": "*"
  }
}
