{
  "name": "@navios/commander",
  "version": "1.5.1",
  "license": "MIT",
  "author": {
    "name": "Oleksandr Hanzha",
    "email": "alex@granted.name"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navios-org/commander.git",
    "directory": "packages/commander"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "typings": "./lib/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./lib/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      }
    },
    "./legacy-compat": {
      "import": {
        "types": "./lib/legacy-compat/index.d.mts",
        "default": "./lib/legacy-compat/index.mjs"
      },
      "require": {
        "types": "./lib/legacy-compat/index.d.cts",
        "default": "./lib/legacy-compat/index.cjs"
      }
    }
  },
  "scripts": {
    "check": "yarn run -T tsgo -b",
    "lint": "yarn run -T oxlint --fix && yarn run -T oxfmt",
    "test": "vitest",
    "test:ci": "vitest run",
    "build": "yarn run -T tsdown",
    "publish:pkg": "yarn npm publish --access public",
    "publish:next": "yarn npm publish --access public --tag next"
  },
  "dependencies": {
    "@navios/core": "^1.0.0"
  },
  "devDependencies": {
    "@navios/builder": "^1.0.0",
    "@navios/commander-tui": "workspace:*",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "zod": "^4.3.5"
  },
  "peerDependencies": {
    "@navios/commander-tui": "^1.0.0",
    "zod": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@navios/commander-tui": {
      "optional": true
    }
  }
}
