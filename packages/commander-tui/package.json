{
  "name": "@navios/commander-tui",
  "version": "1.3.0",
  "license": "MIT",
  "author": {
    "name": "Oleksandr Hanzha",
    "email": "alex@granted.name"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navios-org/commander.git",
    "directory": "packages/commander-tui"
  },
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "typings": "./lib/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./lib/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      }
    },
    "./adapters/react": {
      "import": {
        "types": "./lib/adapters/react/index.d.mts",
        "default": "./lib/adapters/react/index.mjs"
      },
      "require": {
        "types": "./lib/adapters/react/index.d.cts",
        "default": "./lib/adapters/react/index.cjs"
      }
    },
    "./adapters/solid": {
      "import": {
        "types": "./lib/adapters/solid/index.d.mts",
        "default": "./lib/adapters/solid/index.mjs"
      },
      "require": {
        "types": "./lib/adapters/solid/index.d.cts",
        "default": "./lib/adapters/solid/index.cjs"
      }
    }
  },
  "dependencies": {
    "@opentui/core": "^0.1.69",
    "@opentui/react": "^0.1.69",
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.28.6",
    "@navios/core": "^1.0.0",
    "@opentui/solid": "^0.1.69",
    "@types/react": "^19.0.0",
    "rolldown-plugin-solid": "^0.2.1",
    "solid-js": "^1.9.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@navios/core": "*"
  }
}
