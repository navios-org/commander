{
  "name": "@navios/commander-tui",
  "version": "1.6.1",
  "license": "MIT",
  "author": {
    "name": "Oleksandr Hanzha",
    "email": "alex@granted.name"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navios-org/commander.git",
    "directory": "packages/commander-tui"
  },
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "typings": "./lib/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./lib/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      }
    }
  },
  "scripts": {
    "check": "yarn run -T tsgo --build",
    "lint": "yarn run -T oxlint --fix && yarn run -T oxfmt",
    "test": "vitest",
    "test:ci": "vitest run",
    "build": "yarn run -T tsdown",
    "publish:pkg": "yarn npm publish --access public",
    "publish:next": "yarn npm publish --access public --tag next"
  },
  "devDependencies": {
    "@navios/core": "^1.0.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@navios/core": "*"
  }
}
